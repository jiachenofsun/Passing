  m_Script: {fileID: 11500000, guid: a97cf053d6b624025ac64a7ff5339142, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  dialogues:
  - _messages:
    - _actorID: 0
      _message: Oh man oh man oh man...
    - _actorID: 0
      _message: Arthur!
    - _actorID: 0
      _message: ARTHUR!
    - _actorID: 0
      _message: Where are you...
  - _messages:
    - _actorID: 0
      _message: Hey there, have you seen Arthur, my pet turtle?
    - _actorID: 0
      _message: He's a lil cutie and he's got a tophat to boot, ya can't miss him!
  - _messages:
    - _actorID: 0
      _message: I don't know what I would do if I lost Arthur...
    - _actorID: 0
      _message: He's been my best friend since middle school...
  - _messages:
    - _actorID: 0
      _message: Arghh I have no idea where that lil guy went.
    - _actorID: 0
      _message: He does like hanging out in quiet, secluded areas...
  - _messages:
    - _actorID: 0
      _message: Oh my gosh...Arthur, is that you?
    - _actorID: 0
      _message: ARTHUR! Oh thank goodness you're alright!
    - _actorID: 1
      _message: '*turtle noises*'
    - _actorID: 0
      _message: Re-u-ni-ted and it feels so gooood~
    - _actorID: 0
      _message: Thank you so much for finding him!
  - _messages:
    - _actorID: 0
      _message: Forget Lorraine and Bernard, Arthur and I are the town's REAL dynamic
        duo.
    - _actorID: 1
      _message: '*turtle noises*'
  - _messages:
    - _actorID: 0
      _message: Did you know that Arthur is turning 12 tomorrow?
    - _actorID: 0
      _message: What a legend.
    - _actorID: 1
      _message: '*turtle noises*'
  actors:
  - _name: Ellis
    _sprite: {fileID: 21300000, guid: 31b2779aa088140beb9d1c40029856fa, type: 3}
  - _name: Arthur
    _sprite: {fileID: 21300000, guid: 36ec2c520b7004284a7008e6c77ecd48, type: 3}
